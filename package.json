{
  "private": true,
  "packageManager": "pnpm@8.2.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "gen-api": "rm -rf src/api-client && npx @openapitools/openapi-generator-cli generate -i ./spec/schema.yml -g typescript-axios -o src/api-client --additional-properties withSeparateModelsAndApi=true,useSingleRequestParameter=true,apiPackage=api,modelPackage=models"
  },
  "dependencies": {
    "@formkit/icons": "^0.17.5",
    "@formkit/themes": "^0.17.5",
    "@formkit/vue": "^0.17.5",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@headlessui/vue": "^1.7.15",
    "@heroicons/vue": "^2.0.18",
    "@intlify/unplugin-vue-i18n": "^0.10.1",
    "@vueuse/core": "^9.13.0",
    "axios": "^1.4.0",
    "chart.js": "^4.3.2",
    "element-plus": "^2.3.8",
    "eslint-config-prettier": "^8.9.0",
    "js-cookie": "^3.0.5",
    "pinia": "^2.1.6",
    "simplebar-vue3": "^0.2.0",
    "vue": "^3.3.4",
    "vue-good-table-next": "^0.2.1",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4",
    "vue-tippy": "^6.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@iconify-json/carbon": "^1.1.18",
    "@iconify/vue": "^4.1.1",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^18.17.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue-macros/volar": "^0.9.8",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.46.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.16.1",
    "jsdom": "^21.1.2",
    "lint-staged": "^13.2.3",
    "pnpm": "^8.6.11",
    "postcss": "^8.4.27",
    "prettier": "^2.8.8",
    "sass": "^1.64.2",
    "sass-loader": "^13.3.2",
    "simple-git-hooks": "^2.9.0",
    "tailwindcss": "^3.3.3",
    "taze": "^0.9.2",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-macros": "^2.4.0",
    "vite": "^4.4.8",
    "vite-plugin-pages": "^0.29.1",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.8.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "@antfu"
    ]
  },
  "eslintIgnore": [
    "**/*.scss",
    "src/components"
  ]
}
