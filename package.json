{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.10.2",
  "engines": {
    "npm": "8.13.2",
    "node": "17.1.0"
  },
  "scripts": {
    "build": "ENV=production vite-ssg build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer",
    "gen-api": "rm -rf src/api-client && npx @openapitools/openapi-generator-cli generate -i ./spec/schema.yml -g typescript-axios -o src/api-client --additional-properties withSeparateModelsAndApi=true,useSingleRequestParameter=true,apiPackage=api,modelPackage=models"
  },
  "dependencies": {
    "@formkit/icons": "^1.5.9",
    "@formkit/themes": "^1.5.9",
    "@formkit/vue": "^1.5.9",
    "@headlessui/vue": "^1.7.19",
    "@iconify/vue": "^4.1.1",
    "@unhead/vue": "^1.8.11",
    "@vueform/multiselect": "^2.6.6",
    "@vueuse/core": "^10.9.0",
    "@vueuse/head": "^2.0.0",
    "apexcharts": "^3.46.0",
    "date-fns": "^2.30.0",
    "element-plus": "^2.6.0",
    "formkit": "^1.5.9",
    "js-cookie": "^3.0.5",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-apexcharts": "^1.6.2",
    "vue-demi": "^0.14.7",
    "vue-good-table-next": "^0.2.1",
    "vue-i18n": "^9.10.1",
    "vue-router": "^4.3.0",
    "vue-tailwind-datepicker": "^1.7.3",
    "vue-tippy": "^6.4.1",
    "vue-toastification": "2.0.0-rc.5",
    "vue3-apexcharts": "^1.5.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.2.1",
    "@iconify-json/carbon": "^1.1.31",
    "@intlify/unplugin-vue-i18n": "^1.6.0",
    "@types/markdown-it-link-attributes": "^3.0.4",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue-macros/volar": "^0.17.4",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.18",
    "critters": "^0.0.20",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.6",
    "cypress-vite": "^1.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^2.15.1",
    "https-localhost": "^4.7.1",
    "lint-staged": "^15.2.2",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shikiji": "^0.6.13",
    "pnpm": "^8.15.4",
    "postcss": "^8.4.35",
    "rollup": "^4.12.1",
    "sass": "^1.71.1",
    "shikiji": "^0.6.13",
    "simple-git-hooks": "^2.10.0",
    "tailwindcss": "^3.4.1",
    "taze": "^0.12.3",
    "typescript": "^5.4.2",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-macros": "^2.7.10",
    "unplugin-vue-markdown": "^0.24.3",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^5.1.5",
    "vite-bundle-visualizer": "^0.10.1",
    "vite-plugin-inspect": "^0.7.42",
    "vite-plugin-pages": "^0.32.0",
    "vite-plugin-pwa": "^0.16.7",
    "vite-plugin-vue-devtools": "1.0.0-rc.5",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-webfont-dl": "^3.9.1",
    "vite-ssg": "^0.23.6",
    "vite-ssg-sitemap": "^0.6.1",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.27"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
