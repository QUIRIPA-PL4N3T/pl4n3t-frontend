{
  "cSpell.language": "es, en",
  "cSpell.words": [
    "lesspadding",
    "Vitesse",
    "vueuse"
  ],
  "prettier.enable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.associations": {
    "*.css": "postcss"
  },
  "editor.formatOnSave": false,
  "i18n-ally.localesPaths": [
    "locales"
  ]
}
